algoritmo "Exercicio 3"

var
Valor_Produto:Real
Valor_Porcentagem:Inteiro
Valor_Venda:Real
inicio
Valor_Porcentagem <- 35
escreval ("Digite o Valor do produto: ")
Leia (Valor_Produto)
Valor_Venda <- Valor_Porcentagem*Valor_Produto/100+Valor_Produto
Limpatela
escreval ("Valor de venda:" , Valor_Venda:2:2)

fimalgoritmo

